UI menu.c32
LABEL Ubuntu Jammy 22.04 Desktop
	MENU LABEL Ubuntu Desktop
	KERNEL ubuntu/jammy/desktop/vmlinuz
	INITRD ubuntu/jammy/desktop/initrd
	APPEND root=/dev/ram0 ramdisk_size=1500000 ip=dhcp url=http://ocelot/ubuntu/jammy/desktop/ubuntu-22.04.1-desktop-amd64.iso
	TEXT HELP
		The Ubuntu 22.04 Desktop Live Image
	ENDTEXT

