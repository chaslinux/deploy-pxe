	MENU LABEL Ubuntu Server
	KERNEL ubuntu/jammy/server/vmlinuz
	INITRD ubuntu/jammy/server/initrd
	APPEND root=/dev/ram0 ramdisk_size=1500000 ip=dhcp url=http://ocelot/ubuntu/jammy/server/ubuntu-22.04.1-live-server-amd64.iso

